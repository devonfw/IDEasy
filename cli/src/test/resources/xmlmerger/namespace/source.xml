<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root xmlns:merge="https://github.com/devonfw/IDEasy/merge" merge:strategy="COMBINE" xmlns:ns="http://example.com/ns">
  <ns:element name="elem1" attr1="newValue" merge:id="@name"> <!-- strategy inherited from parent!-->
    <ns:child name="child1" attr2="newValue" merge:id="@name">New text</ns:child>
  </ns:element>
  <ns:element name="elem2" attr1="newValue" merge:strategy="OVERRIDE">New text</ns:element>
  <ns:element name="elem3" attr1="newValue" merge:strategy="KEEP"></ns:element>
</root>
