  cp target/ide-projects/corepack/repository/corepack/corepack/default/corepack target/ide-projects/corepack/project/software/node/bin
  cp target/ide-projects/corepack/repository/corepack/corepack/default/corepack target/ide-projects/corepack/project/software/node
fi
